{
  "name": "pet-finder-app",
  "version": "1.0.0",
  "license": "MIT",
  "engines": {
    "node": "18.x"
  },
  "scripts": {
    "dev:api": "nodemon --watch server/**/* -e ts --exec ts-node ./server/index.ts",
    "dev:fe": "parcel serve ./client/index.html",
    "dev:dev": "concurrently \"yarn dev:fe\" \"yarn dev:api\"",
    "dev": "ts-node dev.ts",
    "sync": "ts-node ./server/connection/sync.ts",
    "build:be": "tsc",
    "start": "node ./be-dist/index.js",
    "build:fe": "parcel build ./client/index.html",
    "build": "yarn build:be && yarn build:fe"
  },
  "dependencies": {
    "@sendgrid/mail": "^7.7.0",
    "@vaadin/router": "^1.7.4",
    "algoliasearch": "^4.14.2",
    "cloudinary": "^1.30.1",
    "concurrently": "^7.2.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "dropzone": "^6.0.0-beta.2",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mapbox": "^1.0.0-beta10",
    "mapbox-gl": "^2.9.2",
    "nodemon": "^2.0.19",
    "parcel": "^2.6.2",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "tsc": "^2.0.4",
    "sequelize": "^6.21.3"
  },
  "devDependencies": {
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.0",
    "events": "^3.3.0",
    "https-browserify": "^1.0.0",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "querystring-es3": "^0.2.1",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "url": "^0.11.0"
  }
}
