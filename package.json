  "scripts": {
    "dev:api": "nodemon --watch server/**/* -e ts --exec ts-node ./server/index.ts",
    "dev:fe": "parcel serve ./client/index.html",
    "dev:dev": "concurrently \"yarn dev:fe\" \"yarn dev:api\"",
    "dev": "ts-node dev.ts",
    "sync": "ts-node ./server/sync.ts",
    "build:be": "tsc",
    "start": "node ./be-dist/index.js",
    "build:fe": "parcel build ./client/index.html",
    "build": "yarn build:be && yarn build:fe"
  },
  "dependencies": {
    "@sendgrid/mail": "^7.7.0",
    "@vaadin/router": "^1.7.4",
    "algoliasearch": "^4.14.2",
    "cloudinary": "^1.30.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "concurrently": "^7.2.1",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1",
    "mapbox-gl": "^2.9.2",
    "nodemon": "^2.0.19",
    "parcel": "^2.6.2",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.21.3"
  }
}
